{
  "private": true,
  "watch": {
    "generateReadme": {
      "patterns": [
        "docs",
        "./_README.md"
      ],
      "extensions": "md,mdx"
    }
  },
  "scripts": {
    "generateReadme": "node scripts/convertReadme.js & node node_modules/markdown-include/bin/cli.js markdown.json & node scripts/cleanup.js",
    "docusaurus": "docusaurus",
    "start": "npm-run-all --parallel \"docusaurus start\" watch",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.1",
    "@docusaurus/preset-classic": "2.0.1",
    "@iradics/react-sliderx": "^1.0.6",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "markdown-include": "^0.4.3",
    "npm-watch": "^0.11.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.1",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
